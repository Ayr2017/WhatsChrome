{
    "manifest_version": 1,
    "name": "WhatChapp",
    "description": "WhA parser",
    "version": "1",
    "author": "Ayrat2017",
    "browser_action": {
        "default_icon": "default-icon.png",
        "default_title": "Let's whatch",
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": ["activeTab"]
}